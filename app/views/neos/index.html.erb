<p id="notice"><%= notice %></p>

<h1>Neos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @neos.each do |neo| %>
      <tr>
        <td><%= link_to 'Show', neo %></td>
        <td><%= link_to 'Edit', edit_neo_path(neo) %></td>
        <td><%= link_to 'Destroy', neo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Neo', new_neo_path %>
